digraph DFA {
    rankdir = LR;
    // layout de izquierda a derecha
    
    node [shape = circle;];
    
    // Estado de aceptación.
    F [shape = doublecircle;];
    // estados normales
    DF [shape = doublecircle;];
    // estado de aceptación
    A [shape = circle;];
    B [shape = circle;];
    C [shape = circle;];
    E [shape = circle;];
    
    start [shape = point;];
    // pseudo-nodo para inicio
    start -> A;
    
    A -> B [label = "1/0";];
    A -> C [label = "0/0";];
    B -> C [label = "0/0";];
    B -> A [label = "1/0";];
    C -> C [label = "0/0";];
    C -> DF [label = "1/0";];
    DF -> E [label = "0/0";];
    DF -> A [label = "1/0";];
    E -> C [label = "0/0";];
    E -> DF [label = "1/1";];
    
    start -> 13 [label = "-";];
    13 -> F [label = ">";];
    13 -> F [label = "c - >";];
    start -> 14 [label = "=";];
    start -> F [label = "+, *, /, (, ), {, }, _, ;";];
}