digraph DFA {
    rankdir = LR;
    // layout de izquierda a derecha
    
    node [shape = circle;];
    
    // Estado de aceptación.
    F [shape = doublecircle;];
    
    start [shape = point;];
    // pseudo-nodo para inicio
    
    start -> 13 [label = "\"-\"";];
    13 -> F [label = "\">\"";];
    13 -> F [label = "c - \">\"";];
    start -> 14 [label = "\"=\"";];
    start -> F [label = "\"+\", \"*\", \"/\", \"(\", \")\", \"{\", \"}\", \"_\", \";\"";];
}