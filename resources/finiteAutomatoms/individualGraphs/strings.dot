digraph DFA {
    // Layout más libre y cuadrado
    splines = true;
    overlap = false;
    nodesep = 1;
    ranksep = 1;
    rankdir = LR;
    
    // Estados normales.
    node [shape = circle;];
    
    // Estado de aceptación.
    F [shape = doublecircle;];
    
    
    0 -> 10 [label = "\"";];
    10 -> 10 [label = "L, l";];
    10 -> 11 [label = "\"";];
    11 -> F [label = "C";];
}