digraph DFA {
    // Layout más libre y cuadrado
    splines = true;
    overlap = false;
    nodesep = 1;
    ranksep = 1;
    rankdir = LR;
    
    // Estados normales.
    node [shape = circle;];
    
    // Estado de aceptación.
    F [shape = doublecircle;];
    
    0 -> 9 [label = "\"";];
    9 -> 9 [label = "L, l";];
    9 -> 10 [label = "\"";];
    10 -> F [label = "C";];
}